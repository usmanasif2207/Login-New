<%= form_for @user do |f| %>
<p>
  <%= f.label :user_type %><br />
  <%= f.select(:user_type, options_for_select([['Developer','Developer'],['Manager','Manager'],['QA','QA']]), {prompt: 'Select User'}) %>
</p>
<p>
  <%= f.label :name %><br />
  <%= f.text_field :name %>
</p>
<p>
  <%= f.label :email %><br />
  <%= f.text_field :email %>
</p>
<p>
  <%= f.label :password %><br />
  <%= f.password_field :password %>
</p>

<p><%= f.submit %></p>
<% end %>
